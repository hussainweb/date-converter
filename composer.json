    "date",
    "hijri",
    "calendar"
  ],
  "license": "GPL-2.0",
  "authors": [
    {
      "name": "Hussain Abbas",
      "email": "hussainweb@gmail.com",
      "homepage": "http://hussainweb.me",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.5.0"
  },
  "require-dev": {
    "phpunit/phpunit" : "4.*",
    "squizlabs/php_codesniffer": "~2.3"
  },
  "autoload": {
